{
	"compilerOptions": {
		"module": "commonjs",
		"noImplicitAny": false,
		"sourceMap": true,
		"target": "es6",
		"sourceRoot": "src",
		"outDir": "built",
		"baseUrl": ".",
		"paths": {
			"*": ["node_modules/*"]
		},
		"lib": [
			"dom",
			"es2016"
		],
		"typeRoots": ["node_modules/@types", "types"]
	},
	"include": [
		"**/*"
	]
}
